{
  "Request": {
    "Path": {
      "Matchers": [
        {
          "Name": "WildcardMatcher",
          "Pattern": "/google/*"
        }
      ]
    },
    "Methods": [
      "get"
    ]
  },
  "Response": {
    "UseTransformer": false,
    "ProxyUrl": "http://www.google.com"
  },
  "SaveToFile": true
}